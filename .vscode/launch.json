{
    "configurations": [
        {
            "name": "Cloud Run: CloudRayaAuthTest",
            "type": "cloudcode.cloudrun",
            "request": "launch",
            "build": {
                "buildpacks": {
                    "path": "auth-api/package.json",
                    "builder": "gcr.io/buildpacks/builder:v1"
                }
            },
            "image": "CloudRaya-API",
            "service": {
                "name": "cloud-raya-custom-api",
                "containerPort": 8080,
                "resources": {
                    "limits": {
                        "memory": "256Mi"
                    }
                }
            },
            "target": {
                "minikube": {}
            },
            "watch": true
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start-dev",
            "request": "launch",
            "command": "npm run start-dev",
            "cwd": "${workspaceFolder}\\ml-model"
        }
    ]
}